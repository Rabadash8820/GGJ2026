[attr]lfs filter=lfs diff=lfs merge=lfs -text

## Unity ##

# Scripts
*.cs diff=csharp text
*.shader text

# Unity-specific assets
[attr]unitymerge merge=unityyamlmerge eol=lf
*.anim unitymerge
*.asset unitymerge
*.controller unitymerge
*.mat unitymerge
*.meta unitymerge
*.physicsMaterial unitymerge
*.physicsMaterial2D unitymerge
*.prefab unitymerge
*.unity unitymerge

# Other
*.unitypackage lfs

# Non-Unity-specific assets

# Fonts
*.ttf lfs
*.otf lfs

# Images
*.bmp lfs
*.jpg lfs
*.jpeg lfs
*.png lfs
*.tiff lfs

# Audio
*.mp3 lfs
*.wav lfs

# Plugins
*.dll lfs
*.mdb lfs
*.pdb lfs
*.jar lfs
*.aar lfs
*.dylib lfs

# MS Office Documents
*.xls lfs
*.xlsm lfs
*.xlsx lfs
